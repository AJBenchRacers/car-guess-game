{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm run install:all",
  "outputDirectory": "client/dist",
  "rewrites": [
    { 
      "source": "/api/(.*)", 
      "destination": "/api/$1" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ]
} 